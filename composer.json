{
    "name": "tattali/presigned-url",
    "description": "S3-style presigned URLs for any storage backend",
    "type": "library",
    "license": "MIT",
    "keywords": ["presigned", "url", "storage", "s3", "flysystem", "symfony", "laravel"],
    "authors": [
        {
            "name": "ThÃ©o Attali",
            "email": "theoattali@gmail.com",
            "homepage": "https://theoattali.fr"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.40",
        "illuminate/config": "^12.44",
        "illuminate/container": "^12.44",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "league/flysystem": "^3.0",
        "phpstan/phpstan": "^1.10|^2.0",
        "phpunit/phpunit": "^10.5|^11.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/config": "^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^6.4|^7.0|^8.0",
        "symfony/http-foundation": "^6.4|^7.0|^8.0",
        "symfony/http-kernel": "^6.4|^7.0|^8.0",
        "symfony/routing": "^6.4|^7.0|^8.0"
    },
    "suggest": {
        "symfony/http-foundation": "Required for Symfony integration",
        "symfony/http-kernel": "Required for Symfony bundle",
        "illuminate/support": "Required for Laravel integration",
        "league/flysystem": "Required for Flysystem adapter",
        "aws/aws-sdk-php": "Required for native S3 presigned URLs",
        "psr/log": "Required for access logging"
    },
    "autoload": {
        "psr-4": {
            "Tattali\\PresignedUrl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tattali\\PresignedUrl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tattali\\PresignedUrl\\Bridge\\Laravel\\PresignedUrlServiceProvider"
            ],
            "aliases": {
                "PresignedUrl": "Tattali\\PresignedUrl\\Bridge\\Laravel\\Facades\\PresignedUrl"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
